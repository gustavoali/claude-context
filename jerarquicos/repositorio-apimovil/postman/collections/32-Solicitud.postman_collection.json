									"raw": "{{baseUrl}}/api/gestionSolicitudes/descargarPdfSolicitud/{{idSolicitud}}",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "descargarPdfSolicitud", "{{idSolicitud}}"]
								},
								"description": "Descarga PDF de una solicitud"
							},
							"response": []
