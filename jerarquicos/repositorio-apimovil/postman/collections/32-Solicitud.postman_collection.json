{
	"info": {
		"_postman_id": "sol-032",
		"name": "32 - Solicitud (Gestion Solicitudes)",
		"description": "Endpoints del controller SolicitudController - Gestión de solicitudes de servicios",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"auth": {
		"type": "bearer",
		"bearer": [{"key": "token", "value": "{{token}}", "type": "string"}]
	},
	"item": [
		{
			"name": "Solicitud",
