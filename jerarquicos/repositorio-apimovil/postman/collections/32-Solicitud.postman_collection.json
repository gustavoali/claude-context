					"item": [
						{
							"name": "Guardar Solicitud",
							"request": {
								"method": "POST",
								"header": [{"key": "Content-Type", "value": "application/json"}],
								"body": {
									"mode": "raw",
									"raw": "{\n    \"IdServicio\": 1,\n    \"IdPlan\": {{idPlan}},\n    \"Descripcion\": \"Descripción de la solicitud\",\n    \"Archivos\": []\n}"
								},
								"url": {
									"raw": "{{baseUrl}}/api/gestionSolicitudes/guardarSolicitud",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "guardarSolicitud"]
								},
								"description": "Guarda una nueva solicitud de servicio"
							},
							"response": []
						},
						{
							"name": "Anular Solicitud",
							"request": {
								"method": "GET",
								"header": [],
								"url": {
									"raw": "{{baseUrl}}/api/gestionSolicitudes/anularSolicitud/{{idSolicitud}}",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "anularSolicitud", "{{idSolicitud}}"]
								},
								"description": "Anula una solicitud existente"
							},
							"response": []
						},
						{
							"name": "Guardar Documentacion Faltante",
							"request": {
								"method": "POST",
								"header": [{"key": "Content-Type", "value": "application/json"}],
								"body": {
									"mode": "raw",
									"raw": "{\n    \"IdSolicitud\": {{idSolicitud}},\n    \"Archivos\": []\n}"
