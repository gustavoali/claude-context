				{
					"name": "Consultas",
					"item": [
						{
							"name": "Obtener Informacion Solicitudes",
							"request": {
								"method": "GET",
								"header": [],
								"url": {
									"raw": "{{baseUrl}}/api/gestionSolicitudes/obtenerInformacionSolicitudes",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "obtenerInformacionSolicitudes"]
								},
								"description": "Obtiene información general de solicitudes del socio"
							},
							"response": []
						},
						{
							"name": "Obtener Historial Estado Solicitud",
							"request": {
								"method": "GET",
								"header": [],
								"url": {
									"raw": "{{baseUrl}}/api/gestionSolicitudes/obtenerHistorialEstadoSolicitud/{{idSolicitud}}/{{idServicio}}",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "obtenerHistorialEstadoSolicitud", "{{idSolicitud}}", "{{idServicio}}"]
								},
								"description": "Obtiene el historial de estados de una solicitud específica"
							},
							"response": []
						},
						{
							"name": "Obtener Domicilio y Planes Region",
							"request": {
								"method": "GET",
								"header": [],
								"url": {
									"raw": "{{baseUrl}}/api/gestionSolicitudes/obtenerDomicilioYPlanesRegion",
									"host": ["{{baseUrl}}"],
									"path": ["api", "gestionSolicitudes", "obtenerDomicilioYPlanesRegion"]
								},
								"description": "Obtiene domicilio y planes de la región del socio"
							},
							"response": []
						}
					]
				},
